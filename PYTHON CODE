        return "4750"
    elif value == 19:
        return "4940"
    elif value == 11:
        return "3440"
    elif value == 20:
        return "5130"
    elif value == 0:
        return "150"
    elif value == 15:
        return "350"
    elif value == 25:
        return "650"
    elif value == 35:
        return "950"
    elif value == 45:
        return "1290"
    elif value == 55:
        return "1420"
    elif value == 65:
        return "1550"
    elif value == 75:
        return "1670"
    elif value == 85:
        return "1800"
    elif value == 95:
        return "1930"
    elif value == 105:
        return "3350"
    elif value == 115:
        return "3540"
    elif value == 125:
        return "3720"
    elif value == 135:
        return "3910"
    elif value == 145:
        return "4100"
    elif value == 155:
        return "4280"
    elif value == 165:
        return "4470"
    elif value == 175:
        return "4660"
    elif value == 185:
        return "4840"
    elif value == 195:
        return "5030"
    elif value == 21:
        return "5300"
    else:
        return "No condition met"

# Input from user
value = int(input("Enter a value between 0 and 21: "))

# Calling the function and printing the result
result7 = check_conditions(value)
print(result7)

# Sample string representing a number
string_value = result7

# Convert the string to a float
