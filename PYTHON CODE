# Function to multiply a number by 10% and subtract from the multiplied number
def multiply_and_subtract(result11):
    # Multiply the number by 5%
    result12 = result11 * 0.10
    
    # Subtract the 5% value from the original number
    result13 = result11 - result12
    return result13


# Calling the function and printing the result
result13 = multiply_and_subtract(result11)
print(f"Purchase price {result13}.")
