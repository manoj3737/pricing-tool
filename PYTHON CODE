# Function to check 10 different conditions
def check_conditions(value):
    if value == 1:
        return "200"
    elif value == 2:
        return "500"
    elif value == 3:
        return "800"
    elif value == 4:
        return "1100"
    elif value == 5:
        return "1360"
    elif value == 6:
        return "1480"
    elif value == 7:
        return "1610"
    elif value == 8:
        return "1740"
    elif value == 9:
        return "1860"
    elif value == 10:
        return "3250"
    elif value == 12:
        return "3630"
    elif value == 13:
        return "3820"
    elif value == 14:
        return "4000"
    elif value == 15:
        return "4190"
    elif value == 16:
        return "4380"
