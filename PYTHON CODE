# Function to convert AED to INR
def convert_aed_to_inr(aed_amount, exchange_rate):
    return aed_amount * exchange_rate

# Input from the user
aed_amount = float(input("Enter the amount in AED: "))
exchange_rate = 21  # Example rate, this should be updated with real-time data

# Calling the function and printing the result
inr_amount = convert_aed_to_inr(aed_amount, exchange_rate)
print(f"{aed_amount} AED {inr_amount} INR.")

# Function to multiply a number by 15% and subtract from the multiplied number
def multiply_and_subtract(SW):
    # Multiply the number by 15%
    fifteen_percent = SW * 0.15
    
    # Subtract the 15% value from the original number
    ReturnSW = SW - fifteen_percent
    return ReturnSW 

SW = inr_amount

# Calling the function and printing the result
result1 = multiply_and_subtract(SW)
print(f"The result of multiplying {SW} by 15% and subtracting from {SW} is {result1}.")

# Function to multiply a number by 5% and subtract from result1
def multiply_and_subtract(number1):
    # Multiply the number by 5%
    five_percent = number1 * 0.05
    
    # Subtract the 5% value from result1
    result2 = result1 - five_percent 
    return result2

number1 = inr_amount

# Calling the function and printing the result
result3 = multiply_and_subtract(SW)
print(f"The result of multiplying {number1} by 5% and subtracting from {SW} is {result3}.")

